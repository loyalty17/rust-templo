{
    "name": "gen",
    "about": "Generates a template in a specific folder ",
    "parents": ["tp"],
    "args": {
        "inputs": [
            {
                "name": "template-namespace",
                "help": "Sets the template namespace",
                "required": true
            },
            {
                "name": "output-folder",
                "help": "Sets the output folder",
                "default_value": "./"
            }
        ]
    }
}