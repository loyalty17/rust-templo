{
    "name": "remove",
    "parents": ["tp", "regs"],
    "about": "Removes a remote repo registry",
    "args": {
        "inputs": [
            {
                "name": "remote-repo-name",
                "help": "Sets the remote repo name",
                "required": true
            }  
        ],
        "flags": [
            {
                "long": "yes",
                "short": "y",
                "help": "if is set the registry will be removed without confirmation"
            }
        ]
    }
}