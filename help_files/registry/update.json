{
    "name": "update",
    "parents": ["tp", "regs"],
    "about": "Updates a remote repo registry",
    "args": {
        "inputs": [
            {
                "name": "remote-repo-name",
                "help": "Sets the remote repo name",
                "required": true
            }
        ],
        "options": [
            {
                "long": "name",
                "help": "Sets the new remote repo name",
                "value_name": "new-name"
            },
            {
                "long": "base-url",
                "help": "Sets the new remote repo base url",
                "value_name": "new-b-url"
            },
            {
                "long": "req-auth",
                "help": "Sets if remote repo requires authentication key",
                "value_name": "boolean"
            }
        ]
    }
}