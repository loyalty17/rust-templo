{
    "name": "update",
    "about": "Updates a template from folder",
    "parents": ["tp"],
    "args": {
        "inputs": [
            {
                "name": "template-namespace",
                "help": "Sets the template namespace",
                "required": true
            },
            {
                "name": "folder-path",
                "help": "Sets the the input folder path",
                "default_value": "./"
            }
        ]
    }
}